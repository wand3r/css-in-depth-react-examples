{
  "name": "react-typescript-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf ./build/**.*",
    "compile-webpack-config": "tsc --skipLibCheck webpack.config.ts",
    "build":
      "npm run clean && npm run compile-webpack-config && NODE_ENV=development webpack",
    "build-production":
      "npm run clean && npm run compile-webpack-config && NODE_ENV=production webpack",
    "dev-server":
      "npm run compile-webpack-config && NODE_ENV=development webpack-dev-server",
    "test": "jest",
    "test-watch": "jest --watch",
    "storybook": "start-storybook -p 9001 -c .storybook  -s ./static"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-knobs": "^3.2.16",
    "@storybook/addon-notes": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "@types/html-webpack-plugin": "^2.30.1",
    "@types/jest": "^21.1.6",
    "@types/node": "^8.0.53",
    "@types/react-hot-loader": "^3.0.5",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-knobs": "^3.2.1",
    "@types/storybook__react": "^3.0.5",
    "@types/webpack": "^3.8.1",
    "@types/webpack-dev-server": "^2.9.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^21.2.3",
    "ts-loader": "^3.1.1",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "glamor": "^3.0.0-3",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  }
}
