  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf ./build/**.*",
    "compile-webpack-config": "tsc --allowSyntheticDefaultImports --skipLibCheck webpack.config.ts",
    "build-development": "npm run clean && npm run compile-webpack-config && webpack --env.environment development",
    "build-production": "npm run clean && npm run compile-webpack-config && webpack --env.environment production",
    "build-production-analyze": "npm run clean && npm run compile-webpack-config && webpack --env.environment production --env.analyze",
    "dev-server": "npm run compile-webpack-config && webpack-dev-server --env.environment development",
    "test": "jest",
    "test-watch": "jest --watch",
    "storybook": "start-storybook -p 9001 -c .storybook  -s ./static",
    "cosmos": "npm run compile-webpack-config && cosmos",
    "cosmos:export": "cosmos-export"
  },
